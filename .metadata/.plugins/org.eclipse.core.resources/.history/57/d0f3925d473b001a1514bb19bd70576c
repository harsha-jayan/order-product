/*
 * package com.soagrowers.productquery.mutation;
 * 
 * import org.springframework.beans.factory.annotation.Autowired;
 * 
 * import com.coxautodev.graphql.tools.GraphQLMutationResolver; import
 * com.soagrowers.productquery.domain.Product; import
 * com.soagrowers.productquery.service.ProductService;
 * 
 * public class ProductMutationResolver implements GraphQLMutationResolver {
 * 
 * @Autowired ProductService productService;
 * 
 * public Product updateProductName(final String id, final String name) { return
 * this.productService.updateProduct(id,name); }
 * 
 * }
